N <- seq(100, 50000, by = 100)
M <- rep(0, length(N))
V <- rep(0, length(N))
for (i in 1:length(N)) {
Y <- rnorm(n = N[i], mean = 2, sd = 2)
M[i] = 1 / N[i] * sum(Y)
V[i] = 1 / N[i] * sum((Y - M[i]) ^ 2) # when iid, then var(sum) = sum(var)
}
plot(N, M)
lines(N, rep(2, length(N), type = "l", col = "red"))
